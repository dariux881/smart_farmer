{
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console"],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "ApiConfiguration": {
        "BaseAddress": "http://localhost:5050/"
    },
    "LocalConfiguration": {
        "LocalGrounds" : []
    },
    "AppConfiguration": {
        "OperationalMode" : "Console",
        "PlanCheckCronSchedule": "0/5 * * ? * * *"
    },
    "SerialConfiguration": {
        "SerialPortName": "COM3",
        "BaudRate": 9600,
        "ReadTimeout": 10,
        "WriteTimeout": 10
    }
}